{
  "version" : "0.2.0"
 , "configurations" : [
    {
        "name" : "(gdb) Launch"
      , "type" : "cppdbg"
      , "request" : "launch"
      , "program" : "${workspaceFolder}/build/AlphaFlight"
      , "args" : [ "--invertY" ]
      , "stopAtEntry" : false
      , "cwd" : "${workspaceFolder}/build"
      , "environment" : []
      , "externalConsole" : false
      , "MIMode" : "gdb"
      , "preLaunchTask" : "Build"
      , "setupCommands" : [
        {
            "description" : "Enable pretty-printing for gdb"
          , "text" : "-enable-pretty-printing"
          , "ignoreFailures" : true
        }
      ]
    }
  ]
}
