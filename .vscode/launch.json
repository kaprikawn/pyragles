{
    "version"         : "0.2.0"
  , "configurations"  : [
      {
          "name"            : "(gdb) Launch"
        , "type"            : "cppdbg"
        , "request"         : "launch"
        , "program"         : "${workspaceFolder}/build/pyragles"
        , "args"            : [ "--invertY" ]
        , "stopAtEntry"     : false
        , "cwd"             : "${workspaceFolder}/build"
        , "environment"     : []
        , "externalConsole" : false
        , "MIMode"          : "gdb"
        , "preLaunchTask"   : "cmake build standard"
        ,  "setupCommands"  : [
          {
              "description"     : "Enable pretty-printing for gdb"
            , "text"            : "-enable-pretty-printing"
            , "ignoreFailures"  : true
          }
        ]
      }
